{
  "expo": {
    "name": "Homeal",
    "slug": "homeal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "homeal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF5A1F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.homeal.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF5A1F"
      },
      "package": "com.homeal.app"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-notifications",
      "expo-location"
    ]
  }
}
